{
  "name": "bukharovsi/docker_plugin",
  "type": "composer-plugin",
  "description": "This is plugin for building docker images with composer",
  "license": "Apache 2.0",
  "authors": [
    {
      "name": "Sergey Bukharov",
      "email": "Bukharovsi@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:Bukharovsi/php-shell-wrapper.git"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.1",
    "symfony/console": "^3.2",
    "adambrett/shell-wrapper": "dev-extendingRunnerInterface",
    "league/plates": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "php-mock/php-mock-phpunit": "^1.1",
    "squizlabs/php_codesniffer": "*@dev",
    "pdepend/pdepend": "*@dev",
    "phpmd/phpmd": "*@dev",
    "fzaninotto/faker": "*@dev",
    "mockery/mockery": "*@dev"
  },
  "autoload": {
    "psr-4": {
      "Bukharovsi\\DockerPlugin\\": "src/",
      "Bukharovsi\\DockerPlugin\\Test\\": "test/"
    }
  },
  "extra": {
    "class": "Bukharovsi\\DockerPlugin\\DockerPlugin"
  }
}
